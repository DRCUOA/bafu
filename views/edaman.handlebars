<h1>{{exampleReturn.text}}</h1>

{{#each exampleReturn.hints}}
<div class="food">
  <h2>{{food.label}}</h2>
  <img src="{{food.image}}" alt="{{food.label}}">
  <ul>
    <li>Brand: {{food.brand}}</li>
    <li>Category: {{food.category}}</li>
    <li>Contents: {{food.foodContentsLabel}}</li>
    <li>Calories: {{food.nutrients.ENERC_KCAL}}</li>
    <li>Protein: {{food.nutrients.PROCNT}}</li>
    <li>Fat: {{food.nutrients.FAT}}</li>
    <li>Carbs: {{food.nutrients.CHOCDF}}</li>
    <li>Fiber: {{food.nutrients.FIBTG}}</li>
  </ul>
  <h3>Serving Sizes</h3>
  <ul>
    {{#each food.servingSizes}}
    <li>{{label}} - {{quantity}}</li>
    {{/each}}
  </ul>
  <h3>Measures</h3>
  <ul>
    {{#each measures}}
    <li>{{label}} - {{weight}}</li>
    {{/each}}
  </ul>
</div>
{{/each}}