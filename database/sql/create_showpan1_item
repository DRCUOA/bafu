CREATE TABLE "items" (
	"item_id"	TEXT NOT NULL,
	"created_at"	DATETIME,
	"updated_at"	DATETIME,
	"barcode"	TEXT NOT NULL,
	"item_name"	TEXT NOT NULL,
	"item_description"	TEXT,
	"item_cost"	NUMERIC,
	"item_UOM"	TEXT NOT NULL,
	"item_quantity"	INTEGER NOT NULL,
	"item_img_path"	TEXT,
	PRIMARY KEY("item_id")
) 